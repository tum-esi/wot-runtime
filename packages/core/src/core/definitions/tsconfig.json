{
	"compilerOptions": {
		"target": "es6",
		"lib" : ["es2015","dom"],
		"module": "commonjs",
		"alwaysStrict": true,
		"sourceMap": true,
		"noLib": false,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"strictNullChecks": true
	},
	"exclude": [
		"node_modules",
    ],
	"include": [
		"./Code/**/*"
	]
}
