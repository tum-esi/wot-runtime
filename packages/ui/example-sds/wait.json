    },
    "variables": {},
    "properties": {},
    "actions": {},
    "functions": {},
    "events": {},
    "path": [
        {
            "receive": [
                {
                    "form": {
                        "$ref": "#VirtualCoffeeMachine/events/maintenance/forms/0"
