        },
        {
            "receive": [
                {
                    "form": {
                        "$ref": "#VirtualCoffeeMachine/properties/state/forms/0"
                    },
                    "op": "readproperty"
                }
            ],
            "send": [
                {
                    "form": {
                        "$ref": "#TestThing/properties/string/forms/0"
                    },
                    "op": "writeproperty"
                }
            ],
            "breakOnDataPushed": false
        }
    ]
