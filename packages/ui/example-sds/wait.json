                    "forms": [
                        {
                            "href": "http://127.0.0.1:80/VirtualCoffeeMachine/events/maintenance",
                            "contentType": "application/json",
                            "subprotocol": "longpoll",
                            "op": "subscribeevent"
                        }
                    ]
                }
            }
        },
        "TestThing": {
            "$id": "#TestThing",
            "base": "example://-",
            "properties": {
                "string": {
